  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "copydocs": "./scripts/copydocs.sh",
    "serve": "serve -s build -l 3000",
    "analyze": "BUNDLE_ANALYZER_MODE=server craco build",
    "test:debug": "node --inspect $(yarn bin jest) --no-cache --runInBand",
    "test:dev": "node $(yarn bin jest) --no-cache --runInBand --collectCoverage=false",
    "test:silent": "JEST_SILENT_REPORTER_DOTS=true JEST_SILENT_REPORTER_SHOW_PATHS=true jest --collectCoverage=false --reporters jest-silent-reporter --silent",
    "test:puppeteer": "jest --config ./puppeteer/jest.config.ts --detectOpenHandles",
    "test:snapshots:update": "yarn test:dev -u",
    "test": "jest --coverage --runInBand --silent",
    "posttest": "echo temporarily removed - yarn run test:puppeteer",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules build",
    "type-check": "tsc --noEmit --skipLibCheck",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "vendor-assets": "ts-node ./scripts/vendor-assets.ts",
    "postinstall": "yarn run vendor-assets",
    "prebuild": "yarn run vendor-assets && yarn run copydocs",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
